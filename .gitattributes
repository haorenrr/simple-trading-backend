# Auto detect text files and perform LF normalization
* text=auto

### For springBoot 

/mvnw text eol=lf
*.cmd text eol=crlf

*.java        text eol=lf
*.xml         text eol=lf
*.yml         text eol=lf
*.yaml        text eol=lf
*.properties text eol=lf
*.md          text eol=lf

*.jar         binary
*.min.js      binary

### For vite + vue.js 

# Source code
*.js text eol=lf
*.jsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.vue text eol=lf
*.json text eol=lf
*.mjs text eol=lf
*.cjs text eol=lf

# Styles
*.css text eol=lf
*.scss text eol=lf
*.sass text eol=lf
*.less text eol=lf
*.styl text eol=lf

# HTML/Templates
*.html text eol=lf
*.htm text eol=lf

# Config files
*.yml text eol=lf
*.yaml text eol=lf
*.toml text eol=lf
*.xml text eol=lf
*.ini text eol=lf
*.conf text eol=lf
*.config text eol=lf

# Documentation
*.md text eol=lf
*.txt text eol=lf
*.rtf text eol=lf

# Shell scripts
*.sh text eol=lf
*.bash text eol=lf

# Batch scripts (Windows)
*.bat text eol=crlf
*.cmd text eol=crlf
*.ps1 text eol=crlf

# Git
.gitattributes text eol=lf
.gitignore text eol=lf
.gitconfig text eol=lf

# Package managers
package.json text eol=lf
package-lock.json text eol=lf
yarn.lock text eol=lf
pnpm-lock.yaml text eol=lf

# Images
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.webp binary
*.svg text eol=lf
*.avif binary

# Fonts
*.woff binary
*.woff2 binary
*.eot binary
*.ttf binary
*.otf binary

# Videos
*.mp4 binary
*.mov binary
*.avi binary
*.webm binary

# Audio
*.mp3 binary
*.wav binary
*.ogg binary

# Archives
*.zip binary
*.tar binary
*.gz binary
*.7z binary
*.rar binary

# Documents
*.pdf binary
*.doc binary
*.docx binary
*.xls binary
*.xlsx binary
*.ppt binary
*.pptx binary

# Exclude files from exporting
.gitattributes export-ignore
.gitignore export-ignore
.eslintrc.* export-ignore
.prettierrc.* export-ignore
*.test.js export-ignore
*.spec.js export-ignore
